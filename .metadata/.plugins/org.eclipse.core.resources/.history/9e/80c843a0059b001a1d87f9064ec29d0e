	<insert id="insert" parameterType="com.poi.dto.RoleInsertDTO">
		insert into tbl_role (role_no, member_no)
		values
		<foreach collection="list" item="i">
			(#{member_no},${i})
		 </foreach>
	</insert>
	
</mapper>