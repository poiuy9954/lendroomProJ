package com.poi.mapper;

import org.junit.Test;
import org.springframework.beans.factory.annotation.Autowired;

import com.poi.dto.RoleInsertDTO;
import com.poi.dto.RoleUpDElDTO;
import com.poi.testparent.TestParent;

import lombok.extern.log4j.Log4j;

@Log4j
public class RoleMapperTest extends TestParent {
	
	@Autowired
	private RoleMapper roleMapper;
	
	@Test
	public void test()
	{
		log.info(roleMapper);
	}

	@Test
	public void getList()
	{
		roleMapper.getList(1L).forEach((vo)->log.info(vo));
	}
	
	@Test
	public void insert()
	{
		Long[] role_no = {2L};
		RoleInsertDTO dto = new RoleInsertDTO(5L, role_no);
		log.info(roleMapper.insert(dto));
	}
	
	@Test
	public void update()
	{
		RoleUpDElDTO dto = new RoleUpDElDTO();
		dto.setMember_no(3L);
		dto.setRole_no(1L);
		dto.setChangeRole_no(2L);
		
		log.info(roleMapper.update(dto));
	}
	@Test
	public void delete()
	{
		
	}

}
